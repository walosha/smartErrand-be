{
  "name": "smarterrand",
  "version": "1.0.0",
  "description": "Errand APplication",
  "license": "MIT",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start --watch",
    "start:dev": "concurrently \"npm:start\" \"npm:gen-typings\" \"npm:prisma:generate\"",
    "gen-typings": "ts-node ./src/generate-typings",
    "prisma:generate": "prisma generate --watch",
    "start:debug": "nest start --debug --watch",
    "seed": "ts-node  prisma/seed.ts",
    "db:migrate": "prisma migrate dev",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.2.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^10.0.18",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/mapped-types": "^1.2.2",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.0",
    "@prisma/client": "^4.11.0",
    "@types/bcrypt": "^5.0.0",
    "apollo-server": "3.6.2",
    "apollo-server-express": "^3.6.2",
    "bcrypt": "^5.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "dotenv": "^16.0.0",
    "geojson2h3": "^1.2.0",
    "graphql": "^15.8.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-subscriptions": "2.0.0",
    "h3-js": "^3.7.2",
    "jsonwebtoken": "^9.0.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma": "^4.11.0",
    "prisma-nestjs-graphql": "^17.1.0",
    "referral-codes": "^2.0.3",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "9.0.0",
    "@nestjs/schematics": "9.0.1",
    "@nestjs/testing": "9.0.1",
    "@types/express": "4.17.13",
    "@types/node": "^18.0.3",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "concurrently": "^6.3.0",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "jest": "28.1.2",
    "prettier": "2.7.1",
    "supertest": "6.2.4",
    "ts-jest": "28.0.5",
    "ts-loader": "9.3.1",
    "ts-morph": "15.1.0",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.7.4"
  }
}
